  ): {
    nextIndex: number,
    state: {{{pascalCase tokenizerName}}}TokenizerPreMatchPhaseState,
    nextState?: BlockTokenizerPreMatchPhaseState,
  } | null {
    const { isBlankLine, firstNonWhiteSpaceIndex, endIndex } = eatingInfo
