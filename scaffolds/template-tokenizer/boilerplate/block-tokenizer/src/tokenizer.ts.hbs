    const state: {{{pascalCase tokenizerName}}}TokenizerPreMatchPhaseState = {
      type: {{{pascalCase tokenizerName}}}DataNodeType,
      opening: true,
      parent: parentState,
      content: codePositions.slice(firstNonWhiteSpaceIndex, endIndex),
    }
    return { nextIndex: endIndex, state }
  }

