        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "strong",
                "children": [
                  {
                    "type": "text",
                    "value": "foo "
                  },
                  {
                    "type": "emphasis",
                    "children": [
                      {
                        "type": "text",
                        "value": "bar"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
