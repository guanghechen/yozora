{
  "title": "GFM#428 https://github.github.com/gfm/#example-428",
  "cases": [
    {
      "description": "Indefinite levels of nesting are possible",
      "input": "*foo [*bar*](/url)*",
      "htmlAnswer": "<p><em>foo <a href=\"/url\"><em>bar</em></a></em></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "emphasis",
                "children": [
                  {
                    "type": "text",
                    "value": "foo "
                  },
                  {
                    "type": "link",
                    "url": "/url",
                    "children": [
                      {
                        "type": "emphasis",
                        "children": [
                          {
                            "type": "text",
                            "value": "bar"
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}