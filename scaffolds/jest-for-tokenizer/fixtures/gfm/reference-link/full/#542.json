{
  "title": "GFM#542 https://github.github.com/gfm/#example-542",
  "cases": [
    {
      "description": "The following cases illustrate the precedence of link text grouping over emphasis grouping",
      "input": "*[foo*][ref]\n\n[ref]: /uri",
      "htmlAnswer": "<p>*<a href=\"/uri\">foo*</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "linkDefinition": {
            "ref": {
              "type": "linkDefinition",
              "identifier": "ref",
              "label": "ref",
              "destination": "/uri"
            }
          }
        },
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "text",
                "value": "*"
              },
              {
                "type": "referenceLink",
                "identifier": "ref",
                "label": "ref",
                "referenceType": "full",
                "children": [
                  {
                    "type": "text",
                    "value": "foo*"
                  }
                ]
              }
            ]
          },
          {
            "type": "linkDefinition",
            "identifier": "ref",
            "label": "ref",
            "destination": "/uri"
          }
        ]
      }
    }
  ]
}