      "input": "aaa     \nbbb     ",
      "htmlAnswer": "<p>aaa<br />\nbbb</p>",
      "parseAnswer": {
        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "phrasingContent",
                "contents": [
                  {
                    "type": "TEXT",
                    "content": "aaa     \nbbb"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}