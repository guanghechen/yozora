      "htmlAnswer": "<h2>Foo</h2>",
      "parseAnswer": {
        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "setextHeading",
            "depth": 2,
            "children": [
              {
                "type": "phrasingContent",
                "contents": [
                  {
                    "type": "TEXT",
                    "content": "Foo"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
