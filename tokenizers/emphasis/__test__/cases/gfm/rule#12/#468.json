      "htmlAnswer": "<p><em>foo</em>___</p>",
      "parseAnswer": {
        "type": "root",
        "children": [
          {
            "type": "EMPHASIS_ITALIC",
            "children": [
              {
                "type": "TEXT",
                "value": "foo"
              }
            ]
          },
          {
            "type": "TEXT",
            "value": "___"
          }
        ]
      }
    }
  ]
