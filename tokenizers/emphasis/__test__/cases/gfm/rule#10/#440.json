  "title": "GFM#440 https://github.github.com/gfm/#example-440",
  "cases": [
    {
      "description": "In particular, emphasis and strong emphasis can be nested inside strong emphasis",
      "input": "**foo *bar***",
      "htmlAnswer": "<p><strong>foo <em>bar</em></strong></p>",
      "parseAnswer": {
        "type": "root",
        "children": [
          {
            "type": "EMPHASIS_STRONG",
            "children": [
              {
                "type": "TEXT",
                "value": "foo "
              },
              {
                "type": "EMPHASIS_ITALIC",
                "children": [
                  {
                    "type": "TEXT",
                    "value": "bar"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}