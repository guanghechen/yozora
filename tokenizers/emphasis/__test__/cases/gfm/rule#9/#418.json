  "title": "GFM#418 https://github.github.com/gfm/#example-418",
  "cases": [
    {
      "description": "In particular, emphasis and strong emphasis can be nested inside emphasis",
      "input": "*foo *bar**",
      "htmlAnswer": "<p><em>foo <em>bar</em></em></p>",
      "parseAnswer": {
        "type": "root",
        "children": [
          {
            "type": "emphasis",
            "children": [
              {
                "type": "text",
                "value": "foo "
              },
              {
                "type": "emphasis",
                "children": [
                  {
                    "type": "text",
                    "value": "bar"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}