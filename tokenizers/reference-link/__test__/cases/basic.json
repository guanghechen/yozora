{
  "title": "basic",
  "cases": [
    {
      "description": "mdast# https://github.com/syntax-tree/mdast#linkreference",
      "input": "[alpha][Bravo]\n\n[bravo]: #alpha",
      "parseAnswer": {
        "type": "root",
        "children": [
          {
            "type": "referenceLink",
            "identifier": "bravo",
            "label": "Bravo",
            "referenceType": "full",
            "children": [
              {
                "type": "text",
                "value": "alpha"
              }
            ]
          }
        ]
      }
    },
    {
      "description": "A full reference link could preceded by ]",
      "input": "a][foo][bar]\n\n[foo]: #alpha\n[bar]: #beta",
      "parseAnswer": {
        "type": "root",
        "children": [
          {
            "type": "text",
            "value": "a]"
          },
          {
            "type": "referenceLink",
            "identifier": "bar",
            "label": "bar",
            "referenceType": "full",
            "children": [
              {
                "type": "text",
                "value": "foo"
              }
            ]
          }
        ]
      }
    }
  ]
}
