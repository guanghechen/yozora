      "input": "[foo][bar][baz]\n\n[baz]: /url",
      "htmlAnswer": "<p>[foo]<a href=\"/url\">bar</a></p>",
      "parseAnswer": {
        "type": "root",
        "children": [
          {
            "type": "text",
            "value": "[foo]"
          },
          {
            "type": "referenceLink",
            "identifier": "baz",
            "label": "baz",
            "referenceType": "full",
            "url": "/url",
            "children": [
              {
                "type": "text",
                "value": "bar"
              }
            ]
          }
        ]
      }
    }
  ]
}