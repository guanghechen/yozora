{
  "name": "@yozora/yozora",
  "version": "0.0.0",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/yozora.git"
  },
  "homepage": "https://github.com/lemon-clown/yozora/#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 12.16.0"
  },
  "scripts": {
    ":publish": "lerna publish",
    "pre:publish": "yarn find-inconsistent",
    "bootstrap": "lerna bootstrap",
    "build": "lerna exec --concurrency 1 -- 'echo && pwd && yarn build'",
    "lint:fix": "lerna exec --concurrency 1 -- 'echo && pwd && yarn lint:fix'",
    "clean": "lerna clean --yes && yarn install",
    "rebuild": "lerna exec -- 'rimraf lib/' && yarn build",
    "new:tokenizer": "node ./node_modules/@yozora/template-tokenizer/cli.js",
    "find-inconsistent": "find-inconsistent"
  },
  "workspaces": [
    "packages/*",
    "scaffolds/*",
    "tokenizers/*"
  ],
  "devDependencies": {
    "@barusu/tool-find-inconsistent-packages": "^0.0.3",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.7.6",
    "@yozora/template-tokenizer": "^0.0.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "mocha": "^7.0.0",
    "plop": "^2.6.0",
    "rimraf": "^3.0.2",
    "lerna": "^3.20.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.2"
  }
}
