{
  "name": "@yozora/parser",
  "version": "0.0.0",
  "main": "lib/index.js",
  "files": [
    "lib/",
    ".editorconfig",
    ".eslintrc",
    ".eslintignore",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rimraf lib/ && tsc -b tsconfig.json",
    "postbuild": "rimraf lib/package.json lib/test lib/**/*.tsbuildinfo && mv lib/src/* lib && yarn clean:build",
    "clean:build": "rimraf lib/src/",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "answer": "node -r ts-node/register -r tsconfig-paths/register test/answer.ts",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register test/*.test.ts",
    "prepublishOnly": "yarn lint:fix && yarn test && yarn build"
  },
  "dependencies": {
    "@types/fs-extra": "^8.0.1",
    "@yozora/core": "^0.0.0",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@lemon-clown/mocha-test-master": "^0.0.2"
  }
}
