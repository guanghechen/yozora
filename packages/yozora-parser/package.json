{
  "name": "@yozora/parser",
  "version": "0.0.0",
  "main": "lib/index.js",
  "files": [
    "lib/",
    ".editorconfig",
    ".eslintrc",
    ".eslintignore",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prebuild": "yarn lint:fix && rimraf *.tsbuildinfo lib/",
    "build": "tsc -b tsconfig.json",
    "postbuild": "rimraf lib/package.json lib/test lib/**/*.tsbuildinfo && mv lib/src/* lib && rimraf lib/src/",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prepublishOnly": "yarn build",
    "answer": "node -r ts-node/register -r tsconfig-paths/register test/answer.ts"
  },
  "dependencies": {
    "@types/fs-extra": "^8.0.1",
    "@yozora/core": "^0.0.0",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@yozora/eslint-config": "^0.0.0"
  }
}
