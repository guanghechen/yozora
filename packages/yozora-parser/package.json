{
  "name": "@yozora/parser",
  "version": "0.0.0",
  "main": "lib/index.js",
  "files": [
    "lib/",
    ".editorconfig",
    ".eslintrc",
    ".eslintignore",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rimraf lib/ && tsc -b tsconfig.json",
    "postbuild": "rimraf lib/package.json lib/test lib/**/*.tsbuildinfo && mv lib/src/* lib && yarn clean:build",
    "clean:build": "rimraf lib/src/",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "answer": "node -r ts-node/register -r tsconfig-paths/register test/answer.ts",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register test/*.test.ts",
    "prepublishOnly": "yarn lint:fix && yarn test && yarn build"
  },
  "dependencies": {
    "@types/fs-extra": "^8.0.1",
    "@yozora/core": "^0.0.0",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@lemon-clown/mocha-test-master": "^0.0.2",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "lerna": "^3.20.2",
    "mocha": "^7.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4"
  }
}
