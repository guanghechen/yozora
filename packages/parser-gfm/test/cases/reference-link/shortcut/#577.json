{
  "title": "GFM#577 https://github.github.com/gfm/#example-577",
  "cases": [
    {
      "description": "In the following case [bar][baz] is parsed as a reference, [foo] as normal text",
      "input": "[foo][bar][baz]\n\n[baz]: /url",
      "htmlAnswer": "<p>[foo]<a href=\"/url\">bar</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "LINK_DEFINITION": {
            "baz": {
              "type": "LINK_DEFINITION",
              "identifier": "baz",
              "label": "baz",
              "destination": "/url"
            }
          }
        },
        "children": [
          {
            "type": "PARAGRAPH",
            "children": [
              {
                "type": "PHRASING_CONTENT",
                "contents": [
                  {
                    "type": "TEXT",
                    "value": "[foo]"
                  },
                  {
                    "type": "REFERENCE_LINK",
                    "identifier": "baz",
                    "label": "baz",
                    "referenceType": "full",
                    "url": "/url",
                    "children": [
                      {
                        "type": "TEXT",
                        "value": "bar"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}