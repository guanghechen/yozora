{
  "title": "GFM#573 https://github.github.com/gfm/#example-573",
  "cases": [
    {
      "description": "Full and compact references take precedence over shortcut references",
      "input": "[foo][bar]\n\n[foo]: /url1\n[bar]: /url2",
      "htmlAnswer": "<p><a href=\"/url2\">foo</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "LINK_DEFINITION": {
            "foo": {
              "type": "LINK_DEFINITION",
              "identifier": "foo",
              "label": "foo",
              "destination": "/url1"
            },
            "bar": {
              "type": "LINK_DEFINITION",
              "identifier": "bar",
              "label": "bar",
              "destination": "/url2"
            }
          }
        },
        "children": [
          {
            "type": "PARAGRAPH",
            "children": [
              {
                "type": "PHRASING_CONTENT",
                "contents": [
                  {
                    "type": "REFERENCE_LINK",
                    "identifier": "bar",
                    "label": "bar",
                    "referenceType": "full",
                    "url": "/url2",
                    "children": [
                      {
                        "type": "TEXT",
                        "value": "foo"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}