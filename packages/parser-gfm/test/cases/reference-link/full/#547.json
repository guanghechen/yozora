{
  "title": "GFM#547 https://github.github.com/gfm/#example-547",
  "cases": [
    {
      "description": "Matching is case-insensitive",
      "input": "[foo][BaR]\n\n[bar]: /url \"title\"",
      "htmlAnswer": "<p><a href=\"/url\" title=\"title\">foo</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "LINK_DEFINITION": {
            "bar": {
              "type": "LINK_DEFINITION",
              "identifier": "bar",
              "label": "bar",
              "destination": "/url",
              "title": "title"
            }
          }
        },
        "children": [
          {
            "type": "PARAGRAPH",
            "children": [
              {
                "type": "PHRASING_CONTENT",
                "contents": [
                  {
                    "type": "REFERENCE_LINK",
                    "identifier": "bar",
                    "label": "BaR",
                    "referenceType": "full",
                    "url": "/url",
                    "title": "title",
                    "children": [
                      {
                        "type": "TEXT",
                        "value": "foo"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}