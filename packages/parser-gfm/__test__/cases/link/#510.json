      "htmlAnswer": "<p><a href=\"foo%5Cbar\">link</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "link",
                "url": "foo\\bar",
                "children": [
                  {
                    "type": "text",
                    "value": "link"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}