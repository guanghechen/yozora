            "children": [
              {
                "type": "PHRASING_CONTENT",
                "contents": [
                  {
                    "type": "EMPHASIS_STRONG",
                    "children": [
                      {
                        "type": "TEXT",
                        "value": "foo"
                      }
                    ]
                  },
                  {
                    "type": "TEXT",
                    "value": "*"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
