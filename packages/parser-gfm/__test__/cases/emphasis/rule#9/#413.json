      "parseAnswer": {
        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "phrasingContent",
                "contents": [
                  {
                    "type": "emphasis",
                    "children": [
                      {
                        "type": "text",
                        "value": "foo "
                      },
                      {
                        "type": "link",
                        "url": "/url",
                        "children": [
                          {
                            "type": "text",
                            "value": "bar"
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
