        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "phrasingContent",
                "contents": [
                  {
                    "type": "text",
                    "value": "foo"
                  },
                  {
                    "type": "emphasis",
                    "children": [
                      {
                        "type": "strong",
                        "children": [
                          {
                            "type": "text",
                            "value": "bar"
                          }
                        ]
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "value": "baz"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
