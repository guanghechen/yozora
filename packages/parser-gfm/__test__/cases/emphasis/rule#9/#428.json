      "htmlAnswer": "<p><em>foo <a href=\"/url\"><em>bar</em></a></em></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "PARAGRAPH",
            "children": [
              {
                "type": "PHRASING_CONTENT",
                "contents": [
                  {
                    "type": "EMPHASIS_ITALIC",
                    "children": [
                      {
                        "type": "TEXT",
                        "value": "foo "
                      },
                      {
                        "type": "LINK",
                        "url": "/url",
                        "children": [
                          {
                            "type": "EMPHASIS_ITALIC",
                            "children": [
                              {
                                "type": "TEXT",
                                "value": "bar"
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
