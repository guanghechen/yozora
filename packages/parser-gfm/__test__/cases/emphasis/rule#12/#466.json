      "htmlAnswer": "<p>___<em>foo</em></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {},
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "phrasingContent",
                "contents": [
                  {
                    "type": "text",
                    "value": "___"
                  },
                  {
                    "type": "emphasis",
                    "children": [
                      {
                        "type": "text",
                        "value": "foo"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}