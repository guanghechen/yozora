{
  "title": "GFM#577 https://github.github.com/gfm/#example-577",
  "cases": [
    {
      "description": "In the following case [bar][baz] is parsed as a reference, [foo] as normal text",
      "input": "[foo][bar][baz]\n\n[baz]: /url",
      "htmlAnswer": "<p>[foo]<a href=\"/url\">bar</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "linkDefinition": {
            "baz": {
              "type": "linkDefinition",
              "identifier": "baz",
              "label": "baz",
              "destination": "/url"
            }
          }
        },
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "text",
                "value": "[foo]"
              },
              {
                "type": "referenceLink",
                "identifier": "baz",
                "label": "baz",
                "referenceType": "full",
                "children": [
                  {
                    "type": "text",
                    "value": "bar"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}