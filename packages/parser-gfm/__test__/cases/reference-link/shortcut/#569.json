      "htmlAnswer": "<p><a href=\"/url\" title=\"title\">Foo</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "linkDefinition": {
            "foo": {
              "type": "linkDefinition",
              "identifier": "foo",
              "label": "foo",
              "destination": "/url",
              "title": "title"
            }
          }
        },
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "referenceLink",
                "identifier": "foo",
                "label": "Foo",
                "referenceType": "shortcut",
                "children": [
                  {
                    "type": "text",
                    "value": "Foo"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
