      "input": "[foo][bar][baz]\n\n[baz]: /url1\n[bar]: /url2",
      "htmlAnswer": "<p><a href=\"/url2\">foo</a><a href=\"/url1\">baz</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "linkDefinition": {
            "baz": {
              "type": "linkDefinition",
              "identifier": "baz",
              "label": "baz",
              "destination": "/url1"
            },
            "bar": {
              "type": "linkDefinition",
              "identifier": "bar",
              "label": "bar",
              "destination": "/url2"
            }
          }
        },
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "referenceLink",
                "identifier": "bar",
                "label": "bar",
                "referenceType": "full",
                "url": "/url2",
                "children": [
                  {
                    "type": "text",
                    "value": "foo"
                  }
                ]
              },
              {
                "type": "referenceLink",
                "identifier": "baz",
                "label": "baz",
                "referenceType": "shortcut",
                "url": "/url1",
                "children": [
                  {
                    "type": "text",
                    "value": "baz"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
