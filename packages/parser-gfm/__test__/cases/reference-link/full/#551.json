        "meta": {
          "LINK_DEFINITION": {
            "bar": {
              "type": "LINK_DEFINITION",
              "identifier": "bar",
              "label": "bar",
              "destination": "/url",
              "title": "title"
            }
          }
        },
        "children": [
          {
            "type": "PARAGRAPH",
            "children": [
              {
                "type": "PHRASING_CONTENT",
                "contents": [
                  {
                    "type": "TEXT",
                    "value": "[foo]\n"
                  },
                  {
                    "type": "REFERENCE_LINK",
                    "identifier": "bar",
                    "label": "bar",
                    "referenceType": "shortcut",
                    "url": "/url",
                    "title": "title",
                    "children": [
                      {
                        "type": "TEXT",
                        "value": "bar"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
