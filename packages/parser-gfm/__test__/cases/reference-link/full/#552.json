{
  "title": "GFM#552 https://github.github.com/gfm/#example-552",
  "cases": [
    {
      "description": "When there are multiple matching link reference definitions, the first is used",
      "input": "[foo]: /url1\n\n[foo]: /url2\n\n[bar][foo]",
      "htmlAnswer": "<p><a href=\"/url1\">bar</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "linkDefinition": {
            "foo": {
              "type": "linkDefinition",
              "identifier": "foo",
              "label": "foo",
              "destination": "/url1"
            }
          }
        },
        "children": [
          {
            "type": "paragraph",
            "children": [
              {
                "type": "phrasingContent",
                "contents": [
                  {
                    "type": "referenceLink",
                    "identifier": "foo",
                    "label": "foo",
                    "referenceType": "full",
                    "url": "/url1",
                    "children": [
                      {
                        "type": "text",
                        "value": "bar"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}