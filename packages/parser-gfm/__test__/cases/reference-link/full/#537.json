{
  "title": "GFM#537 https://github.github.com/gfm/#example-537",
  "cases": [
    {
      "description": "The link text may contain balanced brackets, but not unbalanced ones, unless they are escaped",
      "input": "[link \\[bar][ref]\n\n[ref]: /uri",
      "htmlAnswer": "<p><a href=\"/uri\">link [bar</a></p>",
      "parseAnswer": {
        "type": "root",
        "meta": {
          "LINK_DEFINITION": {
            "ref": {
              "type": "LINK_DEFINITION",
              "identifier": "ref",
              "label": "ref",
              "destination": "/uri"
            }
          }
        },
        "children": [
          {
            "type": "PARAGRAPH",
            "children": [
              {
                "type": "PHRASING_CONTENT",
                "contents": [
                  {
                    "type": "REFERENCE_LINK",
                    "identifier": "ref",
                    "label": "ref",
                    "referenceType": "full",
                    "url": "/uri",
                    "children": [
                      {
                        "type": "TEXT",
                        "value": "link [bar"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}