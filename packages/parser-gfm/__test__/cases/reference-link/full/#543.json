              "destination": "/uri"
            }
          }
        },
        "children": [
          {
            "type": "PARAGRAPH",
            "children": [
              {
                "type": "PHRASING_CONTENT",
                "contents": [
                  {
                    "type": "REFERENCE_LINK",
                    "identifier": "ref",
                    "label": "ref",
                    "referenceType": "full",
                    "url": "/uri",
                    "children": [
                      {
                        "type": "TEXT",
                        "value": "foo *bar"
                      }
